{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src" /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "@src/*": ["src/*"],
      "@views/*": ["views/*"],
      "@components/*": ["components/*"],
      "@navigation/*": ["navigation/*"],
      "@models/*": ["models/*"],
      "@store/*": ["store/*"],
      "@networking/*": ["networking/*"],
    },
  }
}
